{
  "name": "fixture-injection",
  "description": "A JavaScript test helper to inject fixtures",
  "license": "MIT",
  "homepage": "https://github.com/yatsu/fixture-injection",
  "repository": {
    "type": "git",
    "url": "https://github.com/yatsu/fixture-injection.git"
  },
  "bugs": {
    "url": "https://github.com/yatsu/fixture-injection/issues"
  },
  "keywords": [
    "fixture",
    "javascript",
    "test"
  ],
  "scripts": {
    "build": "scripty",
    "build:core": "scripty",
    "build:jest": "scripty",
    "clean": "scripty",
    "clean:core": "scripty",
    "clean:jest": "scripty",
    "changelog": "scripty",
    "add:core": "scripty",
    "add:jasmine": "scripty",
    "add:jasmine-tests": "scripty",
    "add:jasmine-example": "scripty",
    "add:jest": "scripty",
    "add:jest-tests": "scripty",
    "add:jest-example": "scripty",
    "add:jest-example-react": "scripty",
    "test": "scripty",
    "test-ci": "scripty",
    "pre-commit": "scripty"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit"
    }
  },
  "scripty": {
    "silent": true
  },
  "engines": {
    "node": ">=10"
  },
  "importSort": {
    ".js, .jsx, .mjs, .ts, .tsx": {
      "parser": "babylon",
      "style": "module"
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "@typescript-eslint/typescript-estree": "^3.7.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "import-sort-cli": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "lerna": "^3.22.1",
    "lerna-changelog": "^1.0.1",
    "lerna-update-wizard": "^0.17.8",
    "prettier": "^2.0.5",
    "scripty": "^2.0.0",
    "typescript": "^3.9.7",
    "typescript-eslint-language-service": "^3.0.0"
  }

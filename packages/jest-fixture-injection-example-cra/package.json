{
  "name": "jest-fixture-injection-example-cra",
  "version": "0.1.4",
  "private": true,
  "dependencies": {
    "@craco/craco": "^3.1.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --env=jest-fixture-injection/jsdom",
    "test-ci": "JEST_JUNIT_OUTPUT=reports/jest/results.xml craco test --coverage --env=jest-fixture-injection/jsdom --ci --reporters=default --reporters=jest-junit --runInBand",
    "eject": "react-scripts eject",
    "pre-commit": "lint-staged && yarn test --coverage"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier-eslint --write",
        "eslint",
        "git add"
      ]
    },
    "ignore": [
      "*.config.js",
      ".eslintrc.js"
    ]
  },
  "author": "fixture-injection Development Team",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "jest-fixture-injection": "^0.1.4",
    "jest-junit": "^5.2.0",
    "lint-staged": "^8.0.5",
    "prettier-eslint-cli": "^4.7.1"
  }
}

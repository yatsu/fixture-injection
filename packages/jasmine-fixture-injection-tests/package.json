{
  "name": "jasmine-fixture-injection-tests",
  "description": "jasmine-fixture-injection tests",
  "version": "0.1.0",
  "scripts": {
    "test-all": "scripty",
    "test-sync": "scripty",
    "test-async": "scripty",
    "pre-commit": "lint-staged && yarn test-all"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier-eslint --write",
        "eslint",
        "git add"
      ]
    },
    "ignore": [
      "*.config.js",
      ".eslintrc.js"
    ]
  },
  "private": true,
  "scripty": {
    "silent": true
  },
  "engines": {
    "node": ">=8"
  },
  "author": "fixture-injection Development Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-node": "^8.0.0",
    "jasmine": "^3.3.0",
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-fixture-injection": "^0.1.0",
    "lint-staged": "^8.0.4",
    "prettier-eslint-cli": "^4.7.1",
    "scripty": "^1.8.0"
  }
}

{
  "name": "jasmine-fixture-injection",
  "description": "fixture-injection helper for Jasmine",
  "version": "0.1.1",
  "scripts": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier-eslint --write",
        "eslint",
        "git add"
      ]
    },
    "ignore": [
      "*.config.js",
      ".eslintrc.js"
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "scripty": {
    "silent": true
  },
  "repository": "https://github.com/yatsu/fixture-injection.git",
  "author": "fixture-injection Development Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yatsu/fixture-injection/issues"
  },
  "homepage": "https://github.com/yatsu/fixture-injection",
  "keywords": [
    "test",
    "fixture",
    "jasmine"
  ],
  "peerDependencies": {
    "jasmine": ">=3 <4"
  },
  "dependencies": {
    "fixture-injection": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-node": "^8.0.0",
    "lint-staged": "^8.0.4",
    "prettier-eslint-cli": "^4.7.1"
  }
}

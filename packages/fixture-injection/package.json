{
  "name": "fixture-injection",
  "description": "A JavaScript test helper to inject fixtures",
  "version": "0.1.0",
  "scripts": {
    "test": "scripty",
    "pre-commit": "lint-staged && yarn test"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier-eslint --write",
        "eslint",
        "git add"
      ]
    },
    "ignore": [
      "*.config.js",
      ".eslintrc.js"
    ]
  },
  "scripty": {
    "silent": true
  },
  "engines": {
    "node": ">=8"
  },
  "repository": "https://github.com/yatsu/fixture-injection.git",
  "author": "fixture-injection Development Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yatsu/fixture-injection/issues"
  },
  "homepage": "https://github.com/yatsu/fixture-injection",
  "keywords": [
    "test",
    "fixture"
  ],
  "dependencies": {
    "es-arguments": "^1.0.2",
    "graph-data-structure": "^1.8.0",
    "node-ipc": "^9.1.1"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-node": "^8.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "prettier-eslint-cli": "^4.7.1",
    "scripty": "^1.8.0"
  }
}

{
  "name": "fixture-injection",
  "description": "A JavaScript test helper to inject fixtures",
  "version": "0.1.4",
  "license": "MIT",
  "homepage": "https://github.com/yatsu/fixture-injection",
  "repository": {
    "type": "git",
    "url": "https://github.com/yatsu/fixture-injection.git"
  },
  "bugs": {
    "url": "https://github.com/yatsu/fixture-injection/issues"
  },
  "keywords": [
    "fixture",
    "javascript",
    "test"
  ],
  "scripts": {
    "test": "scripty",
    "test-ci": "scripty",
    "pre-commit": "lint-staged && yarn test"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier-eslint --write",
        "eslint",
        "git add"
      ]
    },
    "ignore": [
      "*.config.js",
      ".eslintrc.js"
    ]
  },
  "scripty": {
    "silent": true
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "es-arguments": "^1.0.2",
    "graph-data-structure": "^1.8.0",
    "microtime": "^2.1.8",
    "node-ipc": "^9.1.1"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-node": "^8.0.1",
    "jest": "24.8.0",
    "jest-junit": "^5.2.0",
    "lint-staged": "^9.2.0",
    "prettier-eslint-cli": "^5.0.0",
    "scripty": "^1.9.1"
  }
}

      "*.config.js",
      ".eslintrc.js"
    ]
  },
  "private": true,
  "scripty": {
    "silent": true
  },
  "engines": {
    "node": ">=8"
  },
  "author": "fixture-injection Development Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.4",
    "jest-circus": "^23.6.0",
    "nanoid": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-node": "^8.0.0",
    "jest": "^23.6.0",
    "jest-fixture-injection": "^0.1.3",
    "lint-staged": "^8.0.4",
    "prettier-eslint-cli": "^4.7.1",
    "scripty": "^1.8.0"
  }

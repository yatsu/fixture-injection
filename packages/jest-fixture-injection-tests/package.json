{
  "name": "jest-fixture-injection-tests",
  "description": "jest-fixture-injection tests",
  "version": "0.1.3",
  "scripts": {
    "test": "scripty",
    "test-ci": "scripty",
    "test-sync": "scripty",
    "test-async": "scripty",
    "test-reuse": "scripty",
    "test-global-api": "scripty",
    "test-early-teardown": "scripty",
    "pre-commit": "lint-staged && yarn test"
